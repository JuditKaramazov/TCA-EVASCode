{
    "name": "EVA-00",
    "type": "dark",
    "main-Colors": {
        "blue-light": "#A5BDD6",
        "hurtful-blue": "#2D6187",
        "blue": "#759AB4",
        "blue-deep": "#5E81AC",
        "painful-orange": "#F5AD43",
        "red-alert": "#B63958",
        "androgynous-green": "#CBD6D7",
        "serene-pink": "#F5BDDA",
        "orbital-purple": "#9590BD",
        "mindful-gray": "#D8DEE9"
    },
    "colors": {
        "foreground": "#F5BDDA",
        "progressBar.background": "#A5BDD6",
        "focusBorder": "#F5BDDA",
        "selection.background": "#5E81AC",
        "scrollbar.shadow": "#A5BDD6",

        "activityBar.foreground": "#B63958",
